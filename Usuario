class Usuario:
  def __init__(self,nome,email,idade,profissao):
    self.nome= nome
    self.email= email
    self.idade= idade
    self.profissao= profissao
    
  def get_nome(self):
    return(self.nome)
  
  def get_email(self):
    return(self.email)
    
  def get_idade(self):
    return(self.idade)
  
  def get_profissao(self):
    return(self.profissao)
  
  def set_nome(self,nome):
    self.nome= nome
  
  def set_email(self,email):
    self.email= email
  
  def set_idade(self,idade):
    if idade>0:
      self.idade= idade
    elif idade<0:
      pass
  
  def set_profissao(self,profissao):
    self.profissao= profissao
